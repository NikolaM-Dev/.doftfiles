{
  // "files.autoSave": "onFocusChange",
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.sneak": true,
  "vim.easymotionMarkerForegroundColorOneChar": " #68ede0",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#68ede0",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#68ede0",
  "vim.easymotionMarkerBackgroundColor": "#001029",
  // "vim.searchHighlightColor": "#db2a4d",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-n>"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "k"],
      "after": ["{"]
    },
    {
      "before": ["<leader>", "j"],
      "after": ["}"]
    },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previousEditor"]
    }
  ],
  "vim.insertModeKeyBindings": [
    // Scape
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["L", "L"],
      "after": ["<Esc>", "l", "a"]
    },
    {
      "before": ["H", "H"],
      "after": ["<Esc>", "h", "a"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q"]
    },
    {
      "before": ["<leader>", "z"],
      "commands": [":q!"]
    },
    // Copy & Paste
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    },
    // Switch divisions
    {
      "before": ["<C-h>"],
      "after": ["<C-w>", "h"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<C-w>", "j"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<C-w>", "k"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<C-w>", "l"]
    },
    // Tabs
    {
      "before": ["<Leader>", "t", "t"],
      "commands": [":tabnew"]
    },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previusEditor"]
    },
    {
      "before": ["<Leader>", "t", "o"],
      "commands": [":tabo"]
    },
    // Search cleaner
    {
      "before": ["<Leader>", "c"],
      "commands": [":noh"]
    },
    // Comment
    {
      "before": ["<Leader>", "/"],
      "commands": ["editor.action.commentLine"]
    },
    // Comands
    {
      "before": ["<Leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<Leader>", "t"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "h"],
      "after": ["_"]
    },
    {
      "before": ["<leader>", "l"],
      "after": ["$"]
    },
    {
      "before": ["<leader>", "i"],
      "after": ["<C-w>", "s"]
    },
    {
      "before": ["<leader>", "s"],
      "after": ["<C-w>", "v"]
    }
  ],

  "vim.visualModeKeyBindings": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    }
  ],

  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<Leader>", "/"],
      "commands": ["editor.action.commentLine"]
    }
  ],

  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    // VS Code new marker @ next occurence
    "<C-d>": false,
    // Copy
    "<C-c>": false,
    // Cut
    "<C-x>": false,
    // Paste
    "<C-v>": false,
    "<C-z>": false,
    "<C-y>": false
  },

  // Editor
  // "editor.fontFamily": "CaskaydiaCove Nerd Font",
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontWeight": "500",
  "editor.fontSize": 16,
  "editor.fontLigatures": true,
  "prettier.singleQuote": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "workbench.iconTheme": "material-icon-theme",
  // "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontSize": 15,
  "sync.gist": "3cb5c1f60d9fd9e7b2de38a6e807cf2f",
  "workbench.startupEditor": "newUntitledFile",
  "editor.tabSize": 2,
  "liveServer.settings.donotShowInfoMsg": true,
  "explorer.compactFolders": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "liveServer.settings.donotVerifyTags": true,
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/dist"
    }
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.shell.linux": "/usr/bin/zsh",
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "python.showStartPage": false,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "kite.showWelcomeNotificationOnStartup": false,
  "liveshare.authenticationProvider": "GitHub",
  "liveshare.presence": true,
  "workbench.statusBar.visible": true,
  "workbench.activityBar.visible": false,
  "window.menuBarVisibility": "toggle",
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,

  // Golang
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.snippetSuggestions": "none",
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[go.mod]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "gopls": {
    // Add parameter placeholders when completing a function.
    "ui.completion.usePlaceholders": true,

    // If true, enable additional analyses with staticcheck.
    // Warning: This will significantly increase memory usage.
    "ui.diagnostic.staticcheck": false
  },
  "workbench.colorCustomizations": {
    "[Material Theme Ocean High Contrast]": {}
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.autoSave": "onWindowChange",
  "workbench.colorTheme": "Horizon Italic"
}
